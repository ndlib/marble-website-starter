{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "axios": "^0.18.0",
    "fetch": "^1.1.0",
    "gatsby": "^2.5.2",
    "gatsby-image": "^2.0.34",
    "gatsby-node-helpers": "^0.3.0",
    "gatsby-plugin-accessibilityjs": "^1.0.3",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.11",
    "gatsby-plugin-sharp": "^2.0.31",
    "gatsby-plugin-typography": "^2.2.13",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-json": "^2.1.11",
    "gatsby-transformer-remark": "^2.3.10",
    "gatsby-transformer-sharp": "^2.1.17",
    "prop-types": "^15.7.2",
    "query-string": "^6.4.2",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-grid-layout": "^0.16.6",
    "react-helmet": "^5.2.0",
    "react-markdown": "^4.0.8",
    "react-redux": "^7.0.2",
    "react-select": "^2.4.3",
    "react-typography": "^0.16.19",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "typography": "^0.16.19",
    "typy": "^3.0.2"
  },
  "devDependencies": {
    "babel-jest": "^24.7.1",
    "babel-preset-gatsby": "^0.1.11",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint-plugin-react-hooks": "^1.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "jest-environment-enzyme": "^7.0.2",
    "jest-enzyme": "^7.0.2",
    "prettier": "^1.16.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "jest && gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest",
    "watch": "jest --watchAll --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ndlib/marble-website-starter"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/jest/preprocess.js"
    },
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file-mock.js"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "setupFilesAfterEnv": [
      "./jest/enzyme-setup.js"
    ],
    "setupFiles": [
      "<rootDir>/jest/loadershim.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "bugs": {
    "url": "https://github.com/ndlib/marble-website-starter/issues"
  }
}
